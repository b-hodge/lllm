<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <!--<PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %-20c{1.} ::: %msg%n"/>-->
      <PatternLayout pattern="%d{HH:mm:ss}  %-5p  %-30.30c  %msg%n"/>
    </Console>
  </appenders>
  <loggers>
    <root level="debug">
      <appender-ref ref="Console"/>
    </root>
  </loggers>
</configuration>

